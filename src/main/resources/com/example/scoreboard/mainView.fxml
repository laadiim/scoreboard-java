<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxWidth="200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scoreboard.Controller">
   <children>
      <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="164.0">
         <children>
            <Button fx:id="toggle2" mnemonicParsing="false" onAction="#toggle" text="Dva zapasy">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#openSettings" text="Nastaveni">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <VBox alignment="CENTER" prefHeight="170.0" prefWidth="155.0">
         <children>
            <Label alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="164.0" text="Stoly 1 a 2" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label fx:id="lblScore1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="82.0" text="0">
                           <font>
                              <Font size="20.0" />
                           </font></Label>
                        <Button fx:id="btn1HomeAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Add1" prefHeight="25.0" prefWidth="74.0" text="+1" />
                        <Button fx:id="btn1HomeRem" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Rem1" prefHeight="25.0" prefWidth="74.0" text="-1" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefWidth="100.0">
                     <children>
                        <Label fx:id="lblScore2" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="82.0" text="0">
                           <font>
                              <Font size="20.0" />
                           </font></Label>
                        <Button fx:id="btn1AwayAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Add2" prefHeight="25.0" prefWidth="74.0" text="+1">
                           <VBox.margin>
                              <Insets left="4.0" right="4.0" />
                           </VBox.margin></Button>
                        <Button fx:id="btn1AwayRem" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Rem2" prefHeight="25.0" prefWidth="74.0" text="-1" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Button fx:id="zero1" mnemonicParsing="false" onAction="#Zero1" text="Vynulovat">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox fx:id="vbox" alignment="CENTER" prefHeight="170.0" prefWidth="155.0">
         <children>
            <Label alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="164.0" text="Stoly 3 a 4">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER">
                     <children>
                        <Label fx:id="lblScore3" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="82.0" text="0">
                           <font>
                              <Font size="20.0" />
                           </font></Label>
                        <Button fx:id="btn2HomeAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Add3" prefHeight="25.0" prefWidth="74.0" text="+1" />
                        <Button fx:id="btn2HomeRem" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Rem3" prefHeight="25.0" prefWidth="74.0" text="-1" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER">
                     <children>
                        <Label fx:id="lblScore4" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="82.0" text="0">
                           <font>
                              <Font size="20.0" />
                           </font></Label>
                        <Button fx:id="btn2AwayAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Add4" prefHeight="25.0" prefWidth="74.0" text="+1" />
                        <Button fx:id="btn2AwayRem" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#Rem4" prefHeight="25.0" prefWidth="74.0" text="-1" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Button fx:id="zero2" mnemonicParsing="false" onAction="#Zero2" text="Vynulovat">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
